{
  "name": "Ads_for_Good",
  "description": "",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "http://*/*"

  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["insert.js"]
    }
  ],
  "browser_action": {
    "default_title": "Ads_for_Good",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
